tc 14
#AMIC to Receiver codec loopback
!CodecLoop

enable
SLIM_0_TX Channels:One
SLIM TX0 MUX:DEC0
ADC MUX0:AMIC
AMIC MUX0:ADC2
IIR0 INP0 MUX:DEC0
DEC0 Volume:84
ADC2 Volume:0
IIR0 INP0 Volume:84
RX INT0_1 MIX1 INP0:IIR0
SLIM RX0 MUX:AIF_MIX1_PB
RX INT0 DEM MUX:CLSH_DSM_OUT
EAR PA Gain:G_6_DB

disable
AMIC -> Receiver Codec Loopback
SLIM TX0 MUX:0
ADC MUX0:0
AMIC MUX0:0
IIR0 INP0 MUX:0
DEC0 Volume:84
ADC2 Volume:0
SLIM RX0 MUX:0
RX INT0_1 MIX1 INP0:0
RX INT0 DEM MUX:0

