tc 1
#Mic1 to Headset left Afe loopback
!AfeLoop

enable
MultiMedia1 Mixer SLIM_0_TX:1
AIF1_CAP Mixer SLIM TX6:1
SLIM_0_TX Channels:One
SLIM TX6 MUX:DEC6
ADC MUX6:AMIC
AMIC MUX6:ADC1
IIR0 INP0 MUX:DEC6
ADC1 Volume:20
SLIMBUS_DL_HL Switch:1
SLIMBUS_0_RX Audio Mixer MultiMedia1:1
SLIM RX0 MUX:AIF_MIX1_PB
SLIM_0_RX Channels:One
RX INT1_2 MUX:RX0
RX INT1 DEM MUX:CLSH_DSM_OUT
COMP1 Switch:1
Es9018 Bypass Switch:1
SLIMBUS_0_RX Port Mixer SLIM_0_TX:1

disable
MultiMedia1 Mixer SLIM_0_TX:0
AIF1_CAP Mixer SLIM TX6:0
SLIM TX6 MUX:ZERO
ADC MUX6:ZERO
AMIC MUX6:ZERO
IIR0 INP0 MUX:ZERO
ADC1 Volume:0
SLIMBUS_DL_HL Switch:0
SLIMBUS_0_RX Audio Mixer MultiMedia1:0
SLIM RX0 MUX:ZERO
RX INT1_2 MUX:ZERO
RX INT1 DEM MUX:NORMAL_DSM_OUT
COMP1 Switch:0
Es9018 Bypass Switch:0
SLIMBUS_0_RX Port Mixer SLIM_0_TX:0

